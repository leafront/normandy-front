        <div class="navigation">
            <a href="/" class="home_ico"></a>
            <em class="navigation_ico"></em>
            <span>站内信</span>
        </div>
        <div class="content">
            <div class="cont-title">
                <i></i>
                <span>站内信列表</span>
                <a href="#" class="message_read <%if(isRead){%>active<%}%>">全部设为已读</a>
            </div>
            <div class="message">
              <ul class="message_list">
                <% include list.ejs%>
              </ul>
            </div>
            <div class="pagination">
                <ul class="pagination_list">
                  <% include ../common/pagination.ejs %>
                </ul>
            </div>
        </div>
        <% include ../common/footer.ejs%>
    </div>
</div>

<script type="text/javascript">
    window.paginationTpl = JSON.stringify(paginationTpl);
    window.listTpl = <%-JSON.stringify(listTpl)%>;
</script>
<script type="text/javascript" src="/dist/scripts/message/index.js?v=20170806140528"></script>
</body>
</html>

